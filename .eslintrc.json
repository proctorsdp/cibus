{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "eqeqeq": "warn",
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"max-len": "off"
    }
}
